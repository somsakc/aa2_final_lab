@ IN SOA control.example.com. root.control.example.com. (
              3           ;Serial
              3600        ;Refresh
              1800        ;Retry
              604800      ;Expire
              86400       ;Minimum TTL
)
@  IN  NS control.example.com.
 
towerfl	IN A 	192.168.53.12
 
control	IN A 	192.168.50.245
 
satellite	IN A 	192.168.50.177

